{
	"name":"powerful-util",
	"version":"1.0.0-beta.0",
	"description":"工具方法等",
	"main":"lib/index.js",
	"module":"lib/index.js",
	"unpkg":"lib/index.js",
	"jsdelivr":"lib/index.js",
	"types":"index.d.ts",
	"sideEffects":[],
	"scripts":{
		"serve":"powerful-cli serve --mode proxy",
		"build":"powerful-cli build",
		"lib":"npm run lib:esm && npm run lib:node && npm run lib:umd && npm run changelog",
		"lib:esm":"node script/dts.mjs",
		"lib:node":"powerful-cli lib --platform node",
		"lib:umd":"powerful-cli lib --platform umd",
		"test":"jest",
		"browserslist":"browserslist",
		"changelog":"conventional-changelog -p angular -i CHANGELOG.md -s",
		"pre-commit":"lint-staged",
		"prepare":"cd .. && husky front-end/.husky"
	},
	"lint-staged":{
		"*.{js,ts,vue}":[
			"eslint"
		]
	},
	"files":[
		"lib",
		"index.d.ts"
	],
	"exports":{
		".":{
			"types":"./index.d.ts",
			"default":"./lib/index.js"
		}
	},
	"author":"唐国雄",
	"publishConfig":{
		"registry":"https://registry.npmjs.org"
	},
	"engines":{
		"node":">=18.20.1"
	},
	"devDependencies":{
		"@types/jest":"~29.5.14",
		"conventional-changelog-cli":"~5.0.0",
		"eslint-config-powerful":"~1.3.1",
		"husky":"~9.1.7",
		"jest":"~29.7.0",
		"jest-environment-jsdom":"~29.7.0",
		"lint-staged":"~15.2.10",
		"powerful-cli":"~1.13.0",
		"ts-jest":"~29.2.5",
		"typescript":"~5.7.2"
	}
}